{
  "name": "nawpcore",
  "version": "0.2.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "license": "MIT",
  "repository": "https://github.com/MarcFaussurier/NawpCore.git",
  "author": "Faussurier Marc <faussurier.marc@icloud.com>",
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "install": "bower install",
    "start": "php src/Server/Workers/Http.php src/Server/Workers/SocketIO.php",
    "pre-build": "gulp",
    "build-dev": "npm run pre-build && webpack -d --config=configs/webpack/dev.ts",
    "build-prod": "npm run pre-build && webpack -p --config=configs/webpack/prod.ts",
    "watch": "npm run pre-build && concurrently \"npm run watch-client\" \"npm run watch-server\" --kill-others",
    "watch-client": "webpack -d --config=configs/webpack/dev.ts --watch",
    "watch-server": "nodemon --exec \"npm run start\"",
    "lint": "tslint './src/Client/**/*.ts*' --format stylish --force",
    "generate-models": "php ./vendor/atlas/cli/bin/atlas-skeleton.php ./.atlas-config.php && npm run cs-fix",
    "pre-cs-fix": "ts-node configs/precsfix.ts",
    "cs-fix": "npm run pre-cs-fix && php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./bundles && php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./src/Server",
    "test": "php ./vendor/phpunit/phpunit/phpunit --bootstrap tests/bootstrap.php && php ./vendor/jaschilz/php-coverage-badger/coverage-badger.php tests/clover.xml tests/clover.svg"
  },
  "devDependencies": {
    "@types/node": "^10.12.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-header": "^1.8.7",
    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/jquery": "^3.3.22",
    "@types/twig": "^1.12.1",
    "@types/webpack": "^4.4.17",
    "bootstrap": "3.*",
    "bower": "^1.8.4",
    "concurrently": "^4.0.1",
    "copy-dir": "^0.4.0",
    "dotenv": "^6.1.0",
    "jquery": "^3.3.1",
    "morphdom": "^2.3.3",
    "nodemon": "^1.18.4",
    "socket.io-client": "^2.1.1",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "twig": "^1.12.0"
  }
}
