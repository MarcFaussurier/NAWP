{
  "name": "nawpcore",
  "version": "1.0.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "license": "MIT",
  "repository": "https://github.com/MarcFaussurier/NawpCore.git",
  "author": "Faussurier Marc <faussurier.marc@icloud.com>",
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "start": "php src/workers/Http.php src/workers/SocketIO.php",
    "build-dev": "webpack -d --config=configs/webpack/dev.js",
    "build-prod": "webpack -p --config=configs/webpack/prod.js",
    "watch": "concurrently \"npm run watch-client\" \"npm run watch-server\" --kill-others",
    "watch-client": "webpack -d --config=configs/webpack/dev.js --watch",
    "watch-server": "nodemon --exec \"npm run start\"",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "generate-models": "php ./vendor/atlas/cli/bin/atlas-skeleton.php ./atlas-config.php"
  },
  "devDependencies": {
    "@types/node": "^10.7.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/twig": "^1.12.1",
    "concurrently": "^4.0.1",
    "jquery": "^3.3.1",
    "morphdom": "^2.3.3",
    "nodemon": "^1.18.4",
    "socket.io-client": "^1.4",
    "source-map-loader": "^0.2.4",
    "twig": "^1.12.0"
  }
}
